{
  "name": "stalk-fun-monitor",
  "version": "1.0.0",
  "description": "Real-time token monitoring dashboard for stalk.fun",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm run dev",
    "build": "cd client && npm install && npm run build",
    "start": "node server/index.js"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.0",
    "@supabase/supabase-js": "^2.45.6",
    "axios": "^1.7.7",
    "better-sqlite3": "^11.7.0",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "node-fetch": "^3.3.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "concurrently": "^9.1.2"
  }
}